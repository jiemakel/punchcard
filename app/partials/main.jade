.container
  h4 Punchcard
  .form-group
    label Endpoint
    input.form-control(type="text",ng-model="endpoint")
  .form-group
    label Label language preference order
    input.form-control(type="text",ng-model="labelLanguages")
  .form-group
    label Join multiple values using
    input.form-control(type="text",ng-model="joinSeparator")
  yasqe(data="propertiesQuery",endpoint="endpoint",callback="setResults")
  table.table.table-striped
    tr: th(ng-repeat="property in properties")
      .checkbox: label
        input(type="checkbox",ng-model="property.selected")
        | {{property.label}}
      span.label.label-warning(ng-show="property.nonunique != 0 && property.resources") !
    tr(ng-repeat="object in exampleObjects"): td(ng-repeat="value in object") {{value}} 
